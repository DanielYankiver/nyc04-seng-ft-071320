<h1> Edit info of <%= @student.full_name %> </h1>

<%= form_for @student do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br/>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>
  <%= f.label :age %>
  <%= f.number_field :age %><br/>
  <%= f.submit "Ready!" %>
<% end %>